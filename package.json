{
	"name": "lsp-phpunit-server",
	"description": "lsp server for phpunit",
	"version": "0.0.1",
	"author": "recca0120",
	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/recca0120/lsp-phpunit/server"
	},
	"dependencies": {
		"doc-parser": "^0.4.8",
		"php-parser": "git+https://github.com/glayzzle/php-parser.git",
		"vscode-languageserver": "^3.5.0"
	},
	"scripts": {
		"installServer": "installServerIntoExtension ../client ./package.json ./tsconfig.json",
		"compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
		"watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -w -p ."
	}
}
